{
  "name": "spotify",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron && electron . --protocol-launcher",
    "transpile:electron": "tsc --project tsconfig.electron.json",
    "build": "cross-env NODE_ENV=production npm run transpile:electron && vite build",
    "dist:win": "npm run build && electron-builder --win --x64",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "electron-is-dev": "^3.0.1",
    "electron-store": "^10.0.1",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "react": "^19.0.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.5.3",
    "tailwindcss": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^36.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-mkcert": "^1.17.8"
  }
}
